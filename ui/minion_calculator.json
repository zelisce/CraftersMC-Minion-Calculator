                                                   "$toggle_view_binding_name": "beetroot_collection",
                                                   "$hover_text": "§4Beetroot",
                                                   "$texture_default": "textures/items/beetroot.png",
                                                   "$texture_hover": "textures/items/beetroot.png",
                                                   "$texture_pressed": "textures/items/beetroot.png"
                                                }
                                             },
                                             {
                                                "rabbit_collection_tab@minion_calculator.tab_template": {
                                                   "$index": 15,
                                                   "$toggle_name": "minion_collection_tabs",
                                                   "$toggle_view_binding_name": "rabbit_collection",
                                                   "$hover_text": "§pRabbit",
                                                   "$texture_default": "textures/items/rabbit_raw.png",
                                                   "$texture_hover": "textures/items/rabbit_raw.png",
                                                   "$texture_pressed": "textures/items/rabbit_raw.png"
                                                }
                                             },
                                             {
                                                "duck_collection_tab@minion_calculator.tab_template": {
                                                   "$index": 16,
                                                   "$toggle_name": "minion_collection_tabs",
                                                   "$toggle_view_binding_name": "duck_collection",
                                                   "$hover_text": "§qDuck",
                                                   "$texture_default": "textures/ui/minion_types/farming/duck/default.png",
                                                   "$texture_hover": "textures/ui/minion_types/farming/duck/default.png",
                                                   "$texture_pressed": "textures/ui/minion_types/farming/duck/default.png"
                                                }
                                             }
                                          ]
                                       }
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  },
                  {
                     "second_padding": {
                        "type": "panel",
                        "size": [
                           "0px",
                           "10px"
                        ]
                     }
                  },
                  {
                     "minion_upgrades_panel": {
                        "type": "panel",
                        "size": [
                           "100%",
                           "100%cm"
                        ],
                        "controls": [
                           {
                              "stack_panel": {
                                 "type": "stack_panel",
                                 "orientation": "horizontal",
                                 "size": [
                                    "100%c",
                                    "100%cm"
                                 ],
                                 "controls": [
                                    {
                                       "super_compactor@minion_calculator.toggle_template": {
                                          "$toggle_name": "compactors",
                                          "$toggle_view_binding_name": "super_compactor",
                                          "$hover_text": "§1Super Compactor 3000",
                                          "$texture": "textures/ui/minion_types/upgrades/super_compactor.png"
                                       }
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  },
                  {
                     "third_padding": {
                        "type": "panel",
                        "size": [
                           "0px",
                           "10px"
                        ]
                     }
                  },
                  {
                     "minion_tier_slider_panel": {
                        "type": "panel",
                        "size": [
                           "100%",
                           "10px"
                        ],
                        "controls": [
                           {
                              "minion_tier_slider@common.slider": {
                                 "$slider_name": "minion_tier_slider",
                                 "$slider_steps": 11,
                                 "property_bag": {
                                    "#slider_steps": 11,
                                    "#slider_value": 0
                                 },
                                 "size": [
                                    "100% - 20px",
                                    "100%"
                                 ]
                              }
                           }
                        ]
                     }
                  },
                  {
                     "fourth_padding": {
                        "type": "panel",
                        "size": [
                           "0px",
                           "10px"
                        ]
                     }
                  },
                  {
                     "result": {
                        "type": "panel",
                        "size": [
                           "100%",
                           "20px"
                        ],
                        "controls": [
                           {
                              "data_control": {
                                 "type": "panel",
                                 "size": [
                                    "0px",
                                    "0px"
                                 ]
                              }
                           },
                           {
                              "label": {
                                 "type": "label",
                                 "text": "#text",
                                 "color": [
                                    0.3,
                                    0.3,
                                    0.3
                                 ],
                                 "$1twheat": 2880,
                                 "$2twheat": 2880,
                                 "$3twheat": 3323,
                                 "$4twheat": 3323,
                                 "$5twheat": 3927,
                                 "$6twheat": 3927,
                                 "$7twheat": 4320,
                                 "$8twheat": 4320,
                                 "$9twheat": 4800,
                                 "$10twheat": 4800,
                                 "$11twheat": 5400,
                                 "bindings": [
                                    {
                                       "binding_type": "view",
                                       "source_control_name": "wheat_collection",
                                       "source_property_name": "(#toggle_state * 1)",
                                       "target_property_name": "#wheat"
                                    },
                                    {
                                       "binding_type": "view",
                                       "source_control_name": "minion_tier_slider",
                                       "source_property_name": "(#slider_value  * 1 + 1)",
                                       "target_property_name": "#tier"
                                    },
                                    {
                                       "binding_type": "view",
                                       "source_property_name": "('§r' + (#wheat * ((((#tier = 1) * 1) * $1twheat) + (((#tier = 2) * 1) * $2twheat) + (((#tier = 3) * 1) * $3twheat))))",
                                       "target_property_name": "#text"
                                    }
                                 ]
                              }
                           }
                        ]
                     }
                  }
               ]
            }
         }
      ]
   },
   "minion_result_label": {
      "type": "label",
      "$first_tier_amount": 0,
      "$second_tier_amount": 0,
      "$third_tier_amount": 0,
      "$fourth_tier_amount": 0,
      "$fith_tier_amount": 0,
      "$sixth_tier_amount": 0,
      "$seventh_tier_amount": 0,
      "$eighth_tier_amount": 0,
      "$nineth_tier_amount": 0,
      "$tenth_tier_amount": 0,
      "$eleventh_tier_amount": 0,
      "bindings": [
         // {
         //    "binding_type": "view",
         //    "source_property_name": "()"
         // }
      ]
   },
   "tab_template": {
      "type": "panel",
      "$index|default": 0,
      "$toggle_name|default": "none",
      "$texture_default|default": "textures/ui/Black",
      "$texture_hover|default": "textures/ui/Black",
      "$texture_pressed|default": "textures/ui/Black",
      "$hover_text|default": "",
      "$toggle_view_binding_name|default": "none",
      "size": [
         "18px",
         "18px"
      ],
      "controls": [
         {
            "$toggle_view_binding_name@common.radio_toggle": {
               // "$hover_text|default": "",
               "$icon_texture_default|default": "$texture_default",
               "$icon_texture_hover|default": "$texture_hover",
               "$icon_texture_pressed|default": "$texture_pressed",
               "$toggle_group_forced_index": "$index",
               "$toggle_group_default_selected": 0,
               "$unchecked_control|default": "minion_calculator.toggle_unchecked",
               "$checked_control|default": "minion_calculator.toggle_checked",
               "$unchecked_hover_control|default": "minion_calculator.toggle_unchecked_hover",
               "$checked_hover_control|default": "minion_calculator.toggle_checked_hover"
            }
         }
      ]
   },
   "toggle_template": {
      "type": "panel",
      "$toggle_name|default": "none",
      "$texture|default": "textures/ui/Black",
      "$toggle_view_binding_name|default": "none",
      "$hover_text|default": "",
      "size": [
         "18px",
         "18px"
      ],
      "controls": [
         {
            "$toggle_view_binding_name@common.toggle": {
               "$texture_default|default": "$texture",
               "$texture_hover|default": "$texture",
               "$texture_pressed|default": "$texture",
               "$unchecked_control|default": "minion_calculator.toggle_unchecked",
               "$checked_control|default": "minion_calculator.toggle_checked",
               "$unchecked_hover_control|default": "minion_calculator.toggle_unchecked_hover",
               "$checked_hover_control|default": "minion_calculator.toggle_checked_hover",
               "$unchecked_locked_control": "common.empty_panel",
               "$unchecked_locked_hover_control": "common.empty_panel",
               "$checked_locked_control": "common.empty_panel",
               "$checked_locked_hover_control": "common.empty_panel"
            }
         }
      ]
   },
   "toggle_unchecked": {
      "type": "image",
      "texture": "textures/ui/container_slot",
      "controls": [
         {
            "icon": {
               "type": "image",
               "texture": "$texture_default",
               "size": [
                  "100% - 2px",
                  "100% - 2px"
               ]
            }
         }
      ]
   },
   "toggle_checked": {
      "type": "image",
      "texture": "textures/ui/highlight_slot",
      "controls": [
         {
            "focus_border": {
               "type": "image",
               "texture": "textures/ui/focus_border_white"
            }
         },
         {
            "icon": {
               "type": "image",
               "texture": "$texture_pressed",
               "size": [
                  "100% - 2px",
                  "100% - 2px"
               ]
            }
         }
      ]
   },
   "toggle_unchecked_hover": {
      "type": "image",
      "texture": "textures/ui/highlight_slot",
      "controls": [
         {
            "focus_border": {
               "type": "image",
               "texture": "textures/ui/focus_border_white"
            }
         },
         {
            "icon": {
               "type": "image",
               "texture": "$texture_hover",
               "size": [
                  "100% - 2px",
                  "100% - 2px"
               ]
            }
         },
         {
            "hover_text_panel": {
               "type": "custom",
               "renderer": "hover_text_renderer",
               "allow_clipping": false,
               "layer": 20,
               "property_bag": {
                  "#hover_text": "$hover_text"
               }
            }
         }
      ]
   },
   "toggle_checked_hover": {
      "type": "image",
      "texture": "textures/ui/highlight_slot",
      "controls": [
         {
            "focus_border": {
               "type": "image",
               "texture": "textures/ui/focus_border_white"
            }
         },
         {
            "icon": {
               "type": "image",
               "texture": "$texture_pressed",
               "size": [
                  "100% - 2px",
                  "100% - 2px"
               ]
            }
         },
         {
            "hover_text_panel": {
               "type": "custom",
               "renderer": "hover_text_renderer",
               "allow_clipping": false,
               "layer": 20,
               "property_bag": {
                  "#hover_text": "$hover_text"
               }
            }
         }
      ]
   }
}