                                             {
                                                "cow_collection_tab@minion_calculator.tab_template": {
                                                   "$index": 10,
                                                   "$toggle_name": "minion_collection_tabs",
                                                   "$toggle_view_binding_name": "cow_collection",
                                                   "$hover_text": "§mCow",
                                                   "$texture_default": "textures/items/beef_raw.png",
                                                   "$texture_hover": "textures/items/beef_raw.png",
